{
  // Variables: Define reusable images and styles
  "vars": {
    "U_img": "url('data:image/svg+xml;base64,')",
    "L_img": "url('data:image/svg+xml;base64,')",
    "F_img": "url('data:image/svg+xml;base64,')",
    "R_img": "url('data:image/svg+xml;base64,')",
    "B_img": "url('data:image/svg+xml;base64,')",
    "D_img": "url('data:image/svg+xml;base64,')"
  },
  
  // Textures: Compose from vars
  "textures": {
    "U_sprite": {"backgroundImage": "$U_img"},
    "U_full": {"background": "$U_img", "backgroundSize": "cover"},
    "L_sprite": {"backgroundImage": "$L_img"},
    "L_full": {"background": "$L_img", "backgroundSize": "cover"},
    "F_sprite": {"backgroundImage": "$F_img"},
    "F_full": {"background": "$F_img", "backgroundSize": "cover"},
    "R_sprite": {"backgroundImage": "$R_img"},
    "R_full": {"background": "$R_img", "backgroundSize": "cover"},
    "B_sprite": {"backgroundImage": "$B_img"},
    "B_full": {"background": "$B_img", "backgroundSize": "cover"},
    "D_sprite": {"backgroundImage": "$D_img"},
    "D_full": {"background": "$D_img", "backgroundSize": "cover"}
  },
  
  // Cube: Assign textures to faces and stickers
  "cube": {
    // Faces
    "U": "U_sprite",
    "L": "L_sprite",
    "F": "F_sprite",
    "R": "R_sprite",
    "B": "B_sprite",
    "D": "D_sprite",
    
    // Centers
    "U4": "U_full",
    "L4": "L_full",
    "F4": "F_full",
    "R4": "R_full",
    "B4": "B_full",
    "D4": "D_full"
  }
}
